//! # Example of an interface in reproto using the required_fields strategy
//!
//! This is an interface showcasing how the `required_fields` strategy works.

#[type_info(strategy = "required_fields")]
interface RequiredFields {
  shared: string;
  optional_shared?: string;

  A {
    a: string;
    b: string;
    optional_a?: string;
  }

  B {
    a: string;
    optional_b?: string;
  }

  C {
    b: string;
    optional_c?: string;
  }
}
